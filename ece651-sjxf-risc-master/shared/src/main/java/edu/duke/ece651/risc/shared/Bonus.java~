package edu.duke.ece651.risc.shared;

public class Bonus {
  private HashMap<Integer, Integer> bonus;

  public Bonus(){
    this.bonus = new HashMap<>();
    this.bonus.put(0, 0);
    this.bonus.put(1, 1);
    this.bonus.put(2, 3);
    this.bonus.put(3, 5);
    this.bonus.put(4, 8);
    this.bonus.put(5, 11);
    this.bonus.put(6, 15);
  }
}
